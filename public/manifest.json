{
    "manifest_version": 3,
    "name": "Pokernow Bot",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }]
  }